package Immutable;

public class Lab543 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Rajesh r=new Rajesh(10,"Rajesh".9493l);
	}

}

final class Rajesh{
	private final int a;
	private final String b;
	private final long c;
	
	public Rajesh(int a,String b,long c) {
		this.a=a;
		this.b=b;
		this.c=c;
	}
	public int getA() {
		return a;
	}
	public String getB() {
		return b;
	}
	public long getC() {
		return c;
	}
}